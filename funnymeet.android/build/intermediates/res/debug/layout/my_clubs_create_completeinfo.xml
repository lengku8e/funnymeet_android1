<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff8f9fa"
    android:orientation="vertical" >

    <include layout="@layout/somain_title" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:background="#30303538"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/my_clubs_create_complete_fakeicon"
            android:layout_width="90dip"
            android:layout_height="90dip"
            android:background="#ffa3d3ec"
            android:scaleType="centerInside"
            android:src="@drawable/com_tencent_qqpicshow"
            android:visibility="gone" />

        <com.mtcent.funnymeet.ui.view.control.XVURLImageView
            android:id="@+id/my_clubs_create_complete_newicon"
            android:layout_width="90dip"
            android:layout_height="90dip"
            android:scaleType="fitXY"
            android:visibility="visible" />

        <ImageView
            android:id="@+id/my_clubs_create_complete_chooseicon"
            android:layout_width="90dip"
            android:layout_height="90dip"
            android:background="#ffa3d3ec"
            android:scaleType="centerInside"
            android:src="@drawable/com_tencent_qqpicshow" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:layout_marginTop="1dip"
        android:background="#30303538"
        android:gravity="center"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="点击"
            android:textColor="#ff808080"
            android:textSize="12sp" />

        <ImageView
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:scaleType="centerInside"
            android:src="@drawable/com_tencent_qqpicshow" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传自定义俱乐部标志"
            android:textColor="#ff808080"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:background="#ffffffff"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/finditemstyle"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:layout_weight="1"
                android:text="俱乐部类型"
                android:textColor="#ff000000"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:text="私人俱乐部"
                android:textColor="#ff808080"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:scaleType="centerInside"
                android:src="@drawable/gotoright" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#ffaaaaaa" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dip"
        android:layout_marginRight="10dip"
        android:layout_marginTop="10dip"
        android:text="采用管理员邀请制度,获得邀请方可加入"
        android:textColor="#ff808080"
        android:textSize="12sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:layout_marginTop="20dip"
        android:background="#ffffffff"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/finditemstyle"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:layout_weight="1"
                android:text="俱乐部名称"
                android:textColor="#ff000000"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:text="测试俱乐部"
                android:textColor="#ff808080"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:scaleType="centerInside"
                android:src="@drawable/gotoright" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#ffaaaaaa" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/finditemstyle"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:layout_weight="1"
                android:text="俱乐部标签"
                android:textColor="#ff000000"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:text="互联网"
                android:textColor="#ff808080"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:scaleType="centerInside"
                android:src="@drawable/gotoright" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#ffaaaaaa" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/finditemstyle"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:layout_weight="1"
                android:text="俱乐部地点"
                android:textColor="#ff000000"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dip"
                android:layout_marginLeft="15dip"
                android:layout_marginTop="10dip"
                android:text="大连"
                android:textColor="#ff808080"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:scaleType="centerInside"
                android:src="@drawable/gotoright" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#ffaaaaaa" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/finditemstyle"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_marginLeft="15dip"
                android:gravity="center_vertical"
                android:text="介绍"
                android:textColor="#ff000000"
                android:textSize="14sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dip"
                android:layout_marginRight="10dip"
                android:gravity="center_vertical"
                android:text="无"
                android:textColor="#ff808080"
                android:textSize="14sp" />
        </LinearLayout>
    </LinearLayout>

